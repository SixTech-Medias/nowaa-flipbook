#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/vitest@1.1.3_@types+node@20.10.6_jsdom@23.2.0_less@4.2.0_sass@1.69.7_terser@5.26.0/node_modules/vitest/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/vitest@1.1.3_@types+node@20.10.6_jsdom@23.2.0_less@4.2.0_sass@1.69.7_terser@5.26.0/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/vitest@1.1.3_@types+node@20.10.6_jsdom@23.2.0_less@4.2.0_sass@1.69.7_terser@5.26.0/node_modules/vitest/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/vitest@1.1.3_@types+node@20.10.6_jsdom@23.2.0_less@4.2.0_sass@1.69.7_terser@5.26.0/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
