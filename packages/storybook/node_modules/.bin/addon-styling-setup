#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react@18.2.47_less@4.2.0_postcss@8.4.33_react-dom_b6c1c2eaa760a15ab6fddfa7c95c1f54/node_modules/@storybook/addon-styling/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react@18.2.47_less@4.2.0_postcss@8.4.33_react-dom_b6c1c2eaa760a15ab6fddfa7c95c1f54/node_modules/@storybook/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react@18.2.47_less@4.2.0_postcss@8.4.33_react-dom_b6c1c2eaa760a15ab6fddfa7c95c1f54/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react@18.2.47_less@4.2.0_postcss@8.4.33_react-dom_b6c1c2eaa760a15ab6fddfa7c95c1f54/node_modules/@storybook/addon-styling/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react@18.2.47_less@4.2.0_postcss@8.4.33_react-dom_b6c1c2eaa760a15ab6fddfa7c95c1f54/node_modules/@storybook/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react@18.2.47_less@4.2.0_postcss@8.4.33_react-dom_b6c1c2eaa760a15ab6fddfa7c95c1f54/node_modules:/Users/patrickloureiro/Documents/Web/SixTech/Studio/N/Nowaa/web/nowaa-flipbook/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@storybook/addon-styling/postinstall.js" "$@"
else
  exec node  "$basedir/../@storybook/addon-styling/postinstall.js" "$@"
fi
